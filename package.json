{
  "name": "ayaya-league-external",
  "version": "2.0.0",
  "description": "External script platform for League of Legends",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run compile && electron dist/index.js",
    "compile": "tsc --project tsconfig.json",
    "build": "cd src/components/winapi/cpp/ayaya-winapi && node-gyp clean configure build --target=20.0.1 --arch=ia32 --dist-url=https://electronjs.org/headers",
    "compile:cp": "node dev/copy_files.js",
    "package": "node dev/prepare_build.js && electron-builder --win portable --ia32 && node dev/clean_build.js",
    "initplugin": "node dev/create_script.js"
  },
  "build": {
    "appId": "aya.ayayaleague.app",
    "compression": "maximum",
    "asar": true,
    "directories": {
      "output": "build"
    }
  },
  "keywords": [],
  "author": "Emily",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "jsonwebtoken": "^8.5.1",
    "mathjs": "^11.1.0",
    "node-addon-api": "^5.0.0",
    "node-fetch": "^2.6.1",
    "sat": "^0.9.0"
  },
  "devDependencies": {
    "@types/node": "^18.7.13",
    "@types/node-fetch": "^2.6.2",
    "@types/sat": "^0.0.31",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "electron": "^20.0.1",
    "electron-builder": "^23.3.3",
    "electron-packager": "^15.5.1",
    "eslint": "^8.23.1",
    "node-gyp": "^9.1.0",
    "typescript": "^4.8.2"
  },
  "eslintConfig": {
    "env": {
      "es2022": true,
      "node": true,
      "commonjs": true,
      "browser": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "overrides": [],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "linebreak-style": [
        "error",
        "windows"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ],
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/no-empty-function": "off",
      "@typescript-eslint/no-inferrable-types": "off"
    },
    "globals": {
      "manager": "readonly",
      "ctx": "readonly",
      "vKeys": "readonly",
      "register": true,
      "settings": true,
      "getVKEY": true,
      "getSetting": true,
      "Vue": true,
      "ElementPlus": true,
      "loadImage": true
    }
  }
}
